#!/bin/sh
### BEGIN INIT INFO
# Provides:             gowebserver
# Short-Description:    gowebserver test app
# Description:          gowebserver test app for Spinnaker
### END INIT INFO


PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin

set -e

if start-stop-daemon --start --quiet --pidfile /var/run/gowebserver.pid --exec /usr/local/bin/gowebserver
then
	echo "=====> gowebserver started successfully"
else
	echo "=====> gowebserver failed to start"
	exit 1
fi

exit 0
